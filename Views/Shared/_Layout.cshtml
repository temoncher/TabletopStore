<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <!--Import Google Icon Font https://material.io/tools/icons/?style=baseline -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>OnlineStore</title>
</head>
<body>
    <nav class="nav-wrapper blue lighten-1">
        <div class="container">
            <a href="#" class="brand-logo center">Site logo</a>
            <a href="#" class="sidenav-trigger" data-target="mobile-links">
                <i class="material-icons">menu</i>
            </a>
            <ul class="hide-on-med-and-down">
                <li><a href="#">Home</a></li>
                <li><a href="#">Games</a></li>
                <li><a href="#">Categories</a></li>
                <li class="right"><a href="#" class="btn blue lighten-1">Login</a></li>
            </ul>
        </div>
    </nav>

    <ul class="sidenav" id="mobile-links">
        <li><a href="#">Home</a></li>
        <li><a href="#">Games</a></li>
        <li><a href="#">Categories</a></li>
    </ul>
    <div class="container">
        @RenderBody();
    </div>
    <hr />
    <footer>
        <div class="container">
            <p><a href="https://github.com/temoncher/">&copy; 2019 - Temoncher>content</a></p>
        </div>
    </footer>

    <!--JQuery-->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>

    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.sidenav').sidenav()
        })
    </script>
    <script>
        $(document).ready(function () {
            $('.carousel').carousel();
        });
        $('.carousel.carousel-slider').carousel({
            fullWidth: true
        });
    </script>
</body>
</html>
