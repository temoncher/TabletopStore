@model AdminUsersViewModel

<table class="highlight">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Registration date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.RegisterDate</td>
                <td><a class="btn blue lighten-1" asp-action="Profile" asp-route-id="@user.Id">Profile</a></td>
            </tr>
        }
    </tbody>
    <tfoot>
    </tfoot>
</table>